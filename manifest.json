{
  "manifest_version": 3,
  "name": "PDF Dark",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "popups/index.html",
    "default_icon": "images/icon.png"
  },
  "content_scripts": [
    {
      "js": ["content_scripts/index.js"],
      "matches": [
        "file:///*"
      ]
    }
  ]
}
